name: {{appname}}
recipe: drupal9
config:
  php: {{phpversion}}
services:
  appserver:
    webroot: {{webroot}}
    xdebug: true
    config:
      php: .lando/php.ini
    overrides:
      environment:
        PHP_IDE_CONFIG: "serverName=appserver"
        DRUSH_OPTIONS_URI: 'https://{{appname}}.lndo.site'
  solr:
    type: solr:7
    portforward: true
    core: lando
    config:
      dir: .lando/solr_conf
tooling:
  